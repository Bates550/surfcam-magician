{
  "name": "surfcam-magician",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-player": "^1.6.6",
    "react-scripts": "2.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "release": "yarn build && yarn aws:clean && yarn aws:sync && yarn aws:invalidate",
    "aws:clean": "aws s3 rm s3://surfcam-magician --recursive",
    "aws:sync": "aws s3 sync build/ s3://surfcam-magician --acl public-read",
    "aws:invalidate": "aws cloudfront create-invalidation --distribution-id 'ETXUWP3SI20B8' --paths '/index.html'"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        1,
        "always"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
